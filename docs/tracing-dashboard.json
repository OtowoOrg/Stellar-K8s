      "gridPos": { "h": 14, "w": 24, "x": 0, "y": 71 },
      "id": 10,
      "options": {},
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "${DS_TEMPO}" },
          "query": "{ service.name = \"stellar-operator\" && duration > 500ms }",
          "queryType": "traceql",
          "refId": "A"
        }
      ],
      "title": "Slow Spans (>500ms)",
      "type": "traces"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["tracing", "stellar-k8s", "opentelemetry", "tempo"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
